##########################
# Append constant        #
##########################

Given php (Php class file without constant):
  <?php

  declare(strict_types=1);

  namespace App;

  final class Test
  {
  }

Do (Use <C-p>c):
  \<C-p>cpTEST = 1

Expect (Append constant):
  <?php

  declare(strict_types=1);

  namespace App;

  final class Test
  {
      private const TEST = 1;
  }


